!! Noun inflection
!  ---------------
!! The ALBANIAN language nouns inflect in number, case and definiteness.

LEXICON mi
+N+Msc+Sg+Nom+Indef: EndLex ;
+N+Msc+Sg+Nom+Def:%>i EndLex ;
+N+Msc+Sg+Acc+Indef: EndLex ;
+N+Msc+Sg+Acc+Def:%>i EndLex ;
+N+Msc+Sg+Abl+Indef:%>it EndLex ;
+N+Msc+Sg+Abl+Def:%>it EndLex ;
+N+Msc+Sg+Gen+Indef:%>it EndLex ;
+N+Msc+Sg+Gen+Def:%>it EndLex ;
+N+Msc+Sg+Dat+Indef:%>it EndLex ;
+N+Msc+Sg+Dat+Def:%>it EndLex ;

+N+Msc+Pl+Nom+Indef:%>e EndLex ;
+N+Msc+Pl+Nom+Def:%>et EndLex ;
+N+Msc+Pl+Acc+Indef:%>e EndLex ;
+N+Msc+Pl+Acc+Def:%>et EndLex ;
+N+Msc+Pl+Abl+Indef:%>eve EndLex ;
+N+Msc+Pl+Abl+Def:%>eve EndLex ;
+N+Msc+Pl+Gen+Indef:%>eve EndLex ;
+N+Msc+Pl+Gen+Def:%>eve EndLex ;
+N+Msc+Pl+Dat+Indef:%>evet EndLex ;
+N+Msc+Pl+Dat+Def:%>evet EndLex ;


LEXICON mu
+N+Msc+Sg+Nom+Indef: EndLex ;
+N+Msc+Sg+Nom+Def:%>u EndLex ;
+N+Msc+Pl+Nom+Indef:%>j EndLex ;
+N+Msc+Pl+Nom+Def:%>jtÃ« EndLex ;

LEXICON f
+N+Fem+Sg+Nom+Indef: EndLex ;
+N+Fem+Sg+Nom+Def:%>a EndLex ;
+N+Fem+Pl+Nom+Indef:%>a EndLex ;
+N+Fem+Pl+Nom+Def:%>at EndLex ;


